<?xml version="1.0" encoding="UTF-8" ?>
<root>
<!-- START OF PRICE INFO PARAMETERS -->
	<ctryconfig>
		<setting func="currencychar">
			<![CDATA[دولار أمريكي]]>
		</setting>
		<setting func="priceformat">
			<![CDATA[#C# #P#]]>
		</setting>
		<setting func="abbrchars">
			<![CDATA[]]>
		</setting>				
		<setting func="hpstorename">
			<![CDATA[n func="viewshops" type="prima]]>
		</setting>
		<setting func="startpricemultiple">
			<![CDATA[<span class="bold">يبدأ السعر من<br>#PF#<br>#VAT#</span>]]>
		</setting>
		<setting func="startpricesingle">
			<![CDATA[<span class="bold">السعر<br>#PF#<br>#VAT#</span>]]>
		</setting>
	</ctryconfig>
<!-- START OF SCREEN INFO PARAMETERS -->
	<text>
		<field func="intro">
			<![CDATA[مستعد للشراء؟]]>
		</field>
		<field func="promo">
		<![CDATA[HP Best Buy]]> 
		</field>
		<field func="pricedesc">
			<![CDATA[عرض السعر]]>
		</field>
		<field func="pricedescinfo">
			<![CDATA[باستثناء الضرائب المحلية والالتزامات الخاصة المتبعة داخل البلد.]]>
		</field>
		<field func="vat" incl="0">
			<![CDATA[باستثناء الضرائب]]>
		</field>
		<field func="telnr">
			<![CDATA[]]>
		</field>
		<field func="lbintro">
			<![CDATA[Select authorised partner]]>
		</field>
		<field func="warning">
			<![CDATA[Please select a partner]]>
		</field>	
		<field func="stock">
			<![CDATA[خارج المخزن حاليًا]]>
		</field>	
		<field func="bp1">
			<![CDATA[Thank you for selecting an]]>
		</field>		
		<field func="bp2">
			<![CDATA[In 5 seconds you will leave the HP website]]>
		</field>																
	</text>
<!-- START OF CHANNEL EXPERIENCE -->	
	<buttons id="channel">
		<btn func="viewshops" type="primary">
			<label><![CDATA[Authorised HP partner]]></label>
			<txt id="sub">
				<txt><![CDATA[]]></txt>
				<txt><![CDATA[بائعو التجزئة الموصى بهم عبر الإنترنت]]></txt>
			</txt>			
		</btn>
		<btn func="buy" type="commerce">
			<label><![CDATA[Buy now]]></label>
			<action>buy</action>
		</btn>		
		<btn func="findreseller" type="secondary">
			<label><![CDATA[إيجاد شريك محلي]]></label>
			<txt id="sub">
				<txt><![CDATA[إيجاد شريك محلي]]></txt>
			</txt>			
		</btn>			
		<btn func="gonow" type="primary">
			<label><![CDATA[Go now]]></label>
			<action>buy</action>
		</btn>
	</buttons>
<!-- START OF NEUTRAL EXPERIENCE -->	
	<buttons id="neutral">
		<btn func="addbasket" type="primary">
			<label><![CDATA[إضافة إلى السلة]]></label>
			<txt id="sub">
				<txt><![CDATA[الشحن في نفس اليوم من المتجر]]></txt>
				<txt><![CDATA[الشحن خلال أسبوعين عادة]]></txt>
				<txt><![CDATA[الشحن خلال 4 أسابيع عادة]]></txt>								
			</txt>
			<link>
    			<label>Customise this product</label>
			    <action>config</action>
		   	</link> 			
		</btn>	
		<btn func="viewshops" type="secondary">
			<label><![CDATA[الشراء بنظام HP Preferred Partner]]></label>
			<txt id="sub">
				<txt><![CDATA[الشراء بنظام HP Preferred Partner]]></txt>
			</txt>			
		</btn>
		<btn func="buy" type="commerce">
			<label><![CDATA[Buy now]]></label>
			<action>buy</action>
		</btn>		
	    <btn func="customize" type="primary">
	 		<label><![CDATA[Customise]]></label>
	        <txt id="sub">
     		   <txt><![CDATA[Customise and buy directly from HP]]></txt>
        	</txt>
		    <action>config</action>
		</btn>				
		<btn func="findreseller" type="secondary">
			<label><![CDATA[إيجاد شريك محلي]]></label>
			<txt id="sub">
				<txt><![CDATA[إيجاد شريك محلي]]></txt>
			</txt>			
		</btn>			
		<btn func="gonow" type="primary">
			<label><![CDATA[Go now]]></label>
			<action>buy</action>
		</btn>
	</buttons>
<!-- START OF DIRECT EXPERIENCE -->	
	<buttons id="direct">
		<btn func="addbasket" type="commerce">
			<label><![CDATA[إضافة إلى السلة]]></label>
			<txt id="sub">
				<txt><![CDATA[الشحن في نفس اليوم من المتجر]]></txt>
				<txt><![CDATA[الشحن خلال أسبوعين عادة]]></txt>
				<txt><![CDATA[الشحن خلال 4 أسابيع عادة]]></txt>								
			</txt>
			<link>
    			<label>Customise this product</label>
			    <action>config</action>
		   	</link> 			
		</btn>	
		<btn func="basketadded" type="commerce">
			<label><![CDATA[عرض عربة الشراء وفحصها]]></label>
			<txt id="pre">
				<txt><![CDATA[لقد تم إضافة هذا العنصر إلى عربة الشراء]]></txt>
			</txt>
		</btn>
	    <btn func="customize" type="primary">
	 		<label><![CDATA[Customise]]></label>
	        <txt id="sub">
     		   <txt><![CDATA[Customise and buy directly from HP]]></txt>
        	</txt>
		    <action>config</action>
		</btn>				
	</buttons>
<!-- START OF DYNAMIC PARAMETERS -->
	<dynamic>
		<jscontext>
			<text>
				<field func="priceformat" cond="s_ProductLine=KV">
					<![CDATA[]]>
				</field>
			</text>
		</jscontext>
	</dynamic>
</root>
